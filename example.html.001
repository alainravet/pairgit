  box-shadow: rgba(0,0,0,0.2) 0px 1px 3px;
}

div.current {
  background: lightyellow;
}
div.current img {
  -webkit-border-radius:  60px;
  -moz-border-radius:     60px;
  border-radius:          60px;
}

div.current  div.thumbnail {
